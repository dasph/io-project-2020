{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib": ["es2019"],
    "moduleResolution": "node",
    "outDir": "dist",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["ioproject.ts", "webhook.ts"]
}
